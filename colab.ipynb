      "source": [
       "---[ 2. Configuration Widgets ]---"
      ]
     },
     {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {}, // <<< THIS CELL'S CODE IS VISIBLE
      "outputs": [],
      "source": [
       "print(\"--- Configure Analysis Parameters ---\")\n",
       "\n",
       "# Create widgets based on DEFAULT_CONFIG keys we want to expose\n",
       "style = {'description_width': 'initial'} # Allow longer labels\n",
       "\n",
       "# Reference Segments\n",
       "w_sing_ref_time = widgets.FloatText(value=DEFAULT_CONFIG['singing_ref_time'], description='Sing Ref Time (s):', style=style)\n",
       "w_non_sing_ref_time = widgets.FloatText(value=DEFAULT_CONFIG['non_singing_ref_time'], description='Non-Sing Ref (s):', style=style)\n",
       "w_ref_duration = widgets.FloatText(value=DEFAULT_CONFIG['ref_duration'], description='Ref Duration (s):', style=style)\n",
       "\n",
       "# Detection Parameters\n",
       "w_hmm_threshold = widgets.FloatText(value=DEFAULT_CONFIG['hmm_threshold'], description='HMM Threshold:', style=style, step=0.01)\n",
       "w_min_segment_duration = widgets.FloatText(value=DEFAULT_CONFIG['min_segment_duration'], description='Min Seg Dur (s):', style=style)\n",
       "w_min_segment_gap = widgets.FloatText(value=DEFAULT_CONFIG['min_segment_gap'], description='Min Seg Gap (s):', style=style)\n",
       "w_n_components = widgets.IntText(value=DEFAULT_CONFIG['n_components'], description='PCA Components:', style=style)\n",
       "\n",
       "# Identification Parameters\n",
       "w_min_duration_for_id = widgets.FloatText(value=DEFAULT_CONFIG['min_duration_for_id'], description='Min ID Dur (s):', style=style)\n",
       "w_whisper_model = widgets.Dropdown(\n",
       "    options=['tiny', 'base', 'small', 'medium', 'large'],\n",
       "    value=DEFAULT_CONFIG['whisper_model'],\n",
       "    description='Whisper Model:', style=style\n",
       ")\n",
       "w_gemini_api_key = widgets.Password(description='Gemini API Key:', style=style, placeholder='Enter key or leave blank')\n",
       "\n",
       "# Output Settings\n",
       "# Default output dir inside the cloned repo\n",
       "default_output_dir = os.path.join(CLONE_DIR, 'output_colab') \n",
       "w_output_dir = widgets.Text(value=default_output_dir, description='Output Directory:', style=style)\n",
       "w_visualize = widgets.Checkbox(value=DEFAULT_CONFIG['visualize'], description='Visualize Results', indent=False)\n",
       "w_save_json = widgets.Checkbox(value=DEFAULT_CONFIG['save_results_json'], description='Save JSON', indent=False)\n",
       "w_save_csv = widgets.Checkbox(value=DEFAULT_CONFIG['save_results_dataframe'], description='Save CSV', indent=False)\n",
       "\n",
       "# Arrange widgets\n",
       "config_box = widgets.VBox([\n",
       "    widgets.HTML(\"<b>Reference Segments:</b>\"),\n",
       "    widgets.HBox([w_sing_ref_time, w_non_sing_ref_time, w_ref_duration]),\n",
       "    widgets.HTML(\"<hr><b>Detection Parameters:</b>\"),\n",
       "    widgets.HBox([w_hmm_threshold, w_min_segment_duration, w_min_segment_gap, w_n_components]),\n",
       "    widgets.HTML(\"<hr><b>Identification Parameters:</b>\"),\n",
       "    widgets.HBox([w_min_duration_for_id, w_whisper_model]),\n",
       "    w_gemini_api_key,\n",
       "    widgets.HTML(\"<hr><b>Output Settings:</b>\"),\n",
       "    w_output_dir,\n",
       "    widgets.HBox([w_visualize, w_save_json, w_save_csv]),\n",
       "])\n",
       "\n",
       "display(config_box)"
      ]
     },
     {
      "cell_type": "markdown",
      "metadata": {},
      "source": [
       "---[ 3. Input File / URL ]---"
      ]
     },
     {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {}, // <<< THIS CELL'S CODE IS VISIBLE
      "outputs": [],
      "source": [
       "print(\"--- Provide Input Audio ---\")\n",
       "\n",
       "# Option 1: File Upload\n",
       "print(\"Option 1: Upload a local audio file (mp3, wav, m4a, ogg, flac)\")\n",
       "# Use a button to trigger upload and store the result\n",
       "upload_button = widgets.FileUpload(\n",
       "    accept='.mp3,.wav,.m4a,.ogg,.flac', \n",
       "    multiple=False, \n",
       "    description=\"Upload Audio File\"\n",
       ")\n",
       "upload_output = widgets.Output()\n",
       "uploaded_file_path = None # Variable to store the path\n",
       "\n",
       "# Option 2: URL Input\n",
       "w_url = widgets.Text(\n",
       "    description='Option 2: Or Enter URL:', \n",
       "    placeholder='e.g., https://www.youtube.com/watch?v=...', \n",
       "    style={'description_width': 'initial'},\n",
       "    layout=widgets.Layout(width='90%')\n",
       ")\n",
       "\n",
       "display(widgets.VBox([upload_button, upload_output, w_url]))"
      ]
     },
     {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
       "code_folding": "hidden" // <<< THIS CELL'S CODE IS HIDDEN BY DEFAULT
      },
      "outputs": [],
      "source": [
       "# --- Helper Functions (Code Hidden) ---\n",
       "\n",
       "# Global variables to store pipeline instance and results between cells\n",
       "pipeline_instance = None\n",
       "analysis_results = None\n",
       "\n",
       "# Callback for file upload\n",
       "@upload_output.capture(clear_output=True, wait=True)\n",
       "def on_upload_change(change):\n",
       "    global uploaded_file_path\n",
       "    if not change['new']:\n",
       "        uploaded_file_path = None\n",
       "        print(\"Upload cleared.\")\n",
       "        return\n",
       "        \n",
       "    # Get the uploaded file data\n",
       "    uploaded_filename = list(change['new'].keys())[0]\n",
       "    content = change['new'][uploaded_filename]['content']\n",
       "    \n",
       "    # Define where to save the file in Colab's environment\n",
       "    save_dir = '/content/uploads'\n",
       "    os.makedirs(save_dir, exist_ok=True)\n",
       "    uploaded_file_path = os.path.join(save_dir, uploaded_filename)\n",
       "    \n",
       "    # Write the file\n",
       "    with open(uploaded_file_path, 'wb') as f:\n",
       "        f.write(content)\n",
       "    print(f\"File '{uploaded_filename}' uploaded successfully to {uploaded_file_path}\")\n",
       "    # Clear the URL field if a file is uploaded\n",
       "    w_url.value = ''\n",
       "\n",
       "# Attach the callback to the upload button's value property\n",
       "upload_button.observe(on_upload_change, names='value')\n",
       "\n",
       "# --- Callbacks for Pipeline --- \n",
       "status_output = widgets.Output(layout={'border': '1px solid silver', 'max_height': '150px', 'overflow': 'scroll'})\n",
       "progress_bar = widgets.FloatProgress(value=0.0, min=0.0, max=1.0, description='Progress:', bar_style='info')\n",
       "\n",
       "@status_output.capture(clear_output=False, wait=True) # Append messages\n",
       "def colab_status_update(message: str):\n",
       "    """Callback function to display status messages."""\n",
       "    print(f\"[{time.strftime('%H:%M:%S')}] {message}\")\n",
       "\n",
       "def colab_progress_update(value: Optional[float]):\n",
       "    """Callback function to update progress bar."""\n",
       "    if value is None: # Indeterminate\n",
       "        progress_bar.bar_style = 'warning' # Use yellow for indeterminate\n",
       "        progress_bar.value = 1.0 # Fill the bar\n",
       "    else:\n",
       "        progress_bar.bar_style = 'info' # Blue for determinate\n",
       "        progress_bar.value = float(value)\n",
       "\n",
       "# --- Function to gather config from widgets ---\n",
       "def get_config_from_widgets() -> dict:\n",
       "    \"\"\"Reads current values from UI widgets and returns a config dict.\"\"\"\n",
       "    config = {}\n",
       "    # Input Source (priority to uploaded file)\n",
       "    if uploaded_file_path and os.path.exists(uploaded_file_path):\n",
       "        config['file'] = uploaded_file_path\n",
       "        config['url'] = None\n",
       "    elif w_url.value:\n",
       "        config['file'] = None\n",
       "        config['url'] = w_url.value\n",
       "    else:\n",
       "        config['file'] = None\n",
       "        config['url'] = None\n",
       "\n",
       "    # Reference Segments\n",
       "    config['singing_ref_time'] = w_sing_ref_time.value\n",
       "    config['non_singing_ref_time'] = w_non_sing_ref_time.value\n",
       "    config['ref_duration'] = w_ref_duration.value\n",
       "\n",
       "    # Detection Parameters\n",
       "    config['hmm_threshold'] = w_hmm_threshold.value\n",
       "    config['min_segment_duration'] = w_min_segment_duration.value\n",
       "    config['min_segment_gap'] = w_min_segment_gap.value\n",
       "    config['n_components'] = w_n_components.value\n",
       "    config['dim_reduction'] = 'pca' # Assuming fixed\n",
       "\n",
       "    # Identification Parameters\n",
       "    config['min_duration_for_id'] = w_min_duration_for_id.value\n",
       "    config['whisper_model'] = w_whisper_model.value\n",
       "    config['gemini_api_key'] = w_gemini_api_key.value\n",
       "\n",
       "    # Output Settings\n",
       "    config['output_dir'] = w_output_dir.value or default_output_dir # Use default if empty\n",
       "    config['visualize'] = w_visualize.value\n",
       "    config['save_results_json'] = w_save_json.value\n",
       "    config['save_results_dataframe'] = w_save_csv.value\n",
       "    # Use defaults from config.py for filenames\n",
       "    config['results_json_file'] = DEFAULT_CONFIG['results_json_file']\n",
       "    config['results_dataframe_file'] = DEFAULT_CONFIG['results_dataframe_file']\n",
       "\n",
       "    return config"
      ]
     },
     {
      "cell_type": "markdown",
      "metadata": {},
      "source": [
       "---[ 4. Run Analysis Pipeline ]---"
      ]
     },
     {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {}, // <<< THIS CELL'S CODE IS VISIBLE
      "outputs": [],
      "source": [
       "# --- Run Button and Logic ---\n",
       "run_button = widgets.Button(description=\"Start Analysis\", button_style='success', icon='play')\n",
       "stop_button = widgets.Button(description=\"Stop (Not Implemented)\", button_style='danger', icon='stop', disabled=True)\n",
       "\n",
       "def run_analysis(b): # Button click handler\n",
       "    global pipeline_instance, analysis_results\n",
       "    with status_output:\n",
       "        clear_output(wait=True) # Clear previous logs for this run\n",
       "        print(\"--- Starting Analysis --- Please Wait ---\")\n",
       "\n",
       "    run_button.disabled = True\n",
       "    # stop_button.disabled = False # Enable stop if implemented\n",
       "\n",
       "    # Get config right before running\n",
       "    current_config = get_config_from_widgets()\n",
       "\n",
       "    # Basic input validation\n",
       "    if not current_config.get('file') and not current_config.get('url'):\n",
       "        colab_status_update(\"Error: No input file uploaded or URL provided.\")\n",
       "        run_button.disabled = False\n",
       "        return\n",
       "\n",
       "    colab_status_update(f\"Using config: {json.dumps({k:v for k,v in current_config.items() if k != 'gemini_api_key'}, indent=2)}\")\n",
       "    colab_progress_update(0.0) # Reset progress\n",
       "\n",
       "    try:\n",
       "        # Instantiate and run the pipeline\n",
       "        pipeline_instance = SingingAnalysisPipeline(\n",
       "            config=current_config,\n",
       "            status_callback=colab_status_update,\n",
       "            progress_callback=colab_progress_update\n",
       "        )\n",
       "        # The run method is blocking, so Colab cell will wait\n",
       "        analysis_results = pipeline_instance.run()\n",
       "\n",
       "        if analysis_results:\n",
       "            colab_status_update(\"--- Analysis Finished Successfully ---\")\n",
       "            # Trigger saving and visualization if enabled\n",
       "            if pipeline_instance.config.get('save_results_json') or pipeline_instance.config.get('save_results_dataframe'):\n",
       "                 pipeline_instance.save_results()\n",
       "            if pipeline_instance.config.get('visualize'):\n",
       "                 colab_status_update(\"Generating plots...\")\n",
       "                 # This might take a moment and plots appear below the cell\n",
       "                 pipeline_instance.visualize()\n",
       "                 colab_status_update(\"Plotting complete.\")\n",
       "            else:\n",
       "                 colab_status_update(\"Visualization disabled.\")\n",
       "            # Call display_results here to show results immediately after run\n",
       "            display_results()\n",
       "        else:\n",
       "            colab_status_update(\"--- Analysis Failed ---\")\n",
       "\n",
       "    except Exception as e:\n",
       "        colab_status_update(f\"!!! Pipeline Error: {e}\")\n",
       "        import traceback\n",
       "        with status_output:\n",
       "             print(traceback.format_exc())\n",
       "    finally:\n",
       "        run_button.disabled = False\n",
       "        # stop_button.disabled = True\n",
       "        colab_progress_update(0.0) # Reset progress bar visually\n",
       "\n",
       "run_button.on_click(run_analysis)\n",
       "\n",
       "display(widgets.VBox([\n",
       "    widgets.HBox([run_button, stop_button]),\n",
       "    progress_bar,\n",
       "    widgets.HTML(\"<b>Status Log:</b>\"),\n",
       "    status_output # Display status messages here\n",
       "]))\n"
      ]
     },
     {
      "cell_type": "markdown",
      "metadata": {},
      "source": [
       "---[ 5. Display Results ]---"
      ]
     },
     {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {}, // <<< THIS CELL'S CODE IS VISIBLE
      "outputs": [],
      "source": [
       "print(\"\\n--- Analysis Results ---\")\n",
       "results_output = widgets.Output()\n",
       "\n",
       "# Define display_results function (moved definition here)\n",
       "@results_output.capture(clear_output=True, wait=True)\n",
       "def display_results():\n",
       "    # Use global variables as they are updated by the run_analysis function\n",
       "    global analysis_results, pipeline_instance \n",
       "    \n",
       "    if analysis_results:\n",
       "        # Use pipeline_instance if available for config details\n",
       "        source_display = \"N/A\"\n",
       "        output_dir_display = \"N/A\"\n",
       "        if pipeline_instance:\n",
       "             source_display = pipeline_instance.config.get('file') or pipeline_instance.config.get('url')\n",
       "             output_dir_display = pipeline_instance.config.get('output_dir', default_output_dir)\n",
       "        elif analysis_results.get('audio_path'): # Fallback if pipeline_instance not set\n",
       "             source_display = analysis_results.get('audio_path')\n",
       "\n",
       "        print(f\"Analysis completed for source: {source_display}\")\n",
       "        print(f\"Total Duration: {analysis_results.get('total_duration', 'N/A'):.1f}s\")\n",
       "\n",
       "        final_segments = analysis_results.get('final_segments', [])\n",
       "        if final_segments:\n",
       "            print(f\"\\nDetected {len(final_segments)} Singing Segments:\")\n",
       "            segments_df = pd.DataFrame(final_segments, columns=['Start (s)', 'End (s)'])\n",
       "            segments_df['Duration (s)'] = segments_df['End (s)'] - segments_df['Start (s)']\n",
       "            display(segments_df.round(1))\n",
       "\n",
       "            total_singing = segments_df['Duration (s)'].sum()\n",
       "            total_duration = analysis_results.get('total_duration', 0)\n",
       "            percentage = (total_singing / total_duration * 100) if total_duration else 0\n",
       "            print(f\"\\nTotal singing time: {total_singing:.1f}s ({percentage:.1f}%)\")\n",
       "        else:\n",
       "            print(\"\\nNo singing segments detected.\")\n",
       "\n",
       "        id_results = analysis_results.get('identification_results', [])\n",
       "        if id_results:\n",
       "            print(f\"\\nIdentified Songs ({len(id_results)} segments analyzed):\")\n",
       "            # Create DataFrame from identification results\n",
       "            df_data = []\n",
       "            for item in id_results:\n",
       "                 row = {\n",
       "                     'Start': item['segment'][0],\n",
       "                     'End': item['segment'][1],\n",
       "                     'Title': item.get('identification', {}).get('title', 'N/A'),\n",
       "                     'Artist': item.get('identification', {}).get('artist', 'N/A'),\n",
       "                     'Confidence': item.get('identification', {}).get('confidence', 'N/A'),\n",
       "                     'Transcript Snippet': item.get('transcript', '')[:100] + ('...' if len(item.get('transcript', '')) > 100 else '')\n",
       "                 }\n",
       "                 df_data.append(row)\n",
       "            id_df = pd.DataFrame(df_data)\n",
       "            display(id_df.round(1))\n",
       "        elif final_segments: # Only mention if segments existed but none were ID'd\n",
       "             print(\"\\nNo songs identified (segments might be too short or identification failed).\")\n",
       "\n",
       "        # Plots should have appeared in the output of the previous cell if visualize=True\n",
       "        if pipeline_instance and pipeline_instance.config.get('visualize'):\n",
       "             print(\"\\nVisualizations should be displayed above or in the previous cell's output.\")\n",
       "\n",
       "        # Output file location reminder\n",
       "        print(f\"\\nOutput files (if saved) are in: {output_dir_display}\")\n",
       "        print(\"(Use the Colab file browser on the left to download.)\")\n",
       "\n",
       "    else:\n",
       "        print(\"No analysis results available. Run the pipeline first.\")\n",
       "\n",
       "# Display the results area - it will be populated when display_results() is called\n",
       "display(results_output)\n",
       "\n",
       "# Call it once initially to show the 'No results' message or previous results\n",
       "display_results()"
      ]
     }
